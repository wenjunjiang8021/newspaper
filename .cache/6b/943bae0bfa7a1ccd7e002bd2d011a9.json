l":{"line":383,"column":8},"generated":{"line":296,"column":2}},{"source":"css/styles.scss","name":null,"original":{"line":384,"column":8},"generated":{"line":297,"column":2}},{"source":"css/styles.scss","name":null,"original":{"line":386,"column":6},"generated":{"line":299,"column":0}},{"source":"css/styles.scss","name":null,"original":{"line":387,"column":8},"generated":{"line":300,"column":2}},{"source":"css/styles.scss","name":null,"original":{"line":388,"column":8},"generated":{"line":301,"column":2}},{"source":"css/styles.scss","name":null,"original":{"line":389,"column":8},"generated":{"line":302,"column":2}},{"source":"css/styles.scss","name":null,"original":{"line":390,"column":8},"generated":{"line":303,"column":2}}],"sources":{"css/styles.scss":"/* $breakpoint-phone: 576px; //or 480? auto\n$breakpoint-tablet: 768px; //750\n$breakpoint-laptop: 992px; //960\n$breakpoint-desktop: 1200px; //1140 */\n:root {\n    --c1:#e9ecf2;\n    --c2:#a3b2bf;\n    --c3:#10261e;\n    --c4:#2e594a;\n    --c5:#3d735f;\n  }\n  \n  $theme-black: #111111;\n  /*\n  Screen sizes:\n    xs: 0 - 480px;\n    sm: 480.1px - 768px;\n    md: 768.1px - 992px;\n    lg: 992.1ps - 1200px;\n    xl: 1200.1px and above\n  */\n  $breakpoints: 480px, 768px, 992px, 1200px;\n  @mixin screen($width, $type: 'only') {\n    $index: 0;\n    @if $width == 'xs' {\n        $index: 1;\n    } @else if $width == 'sm' {\n        $index: 2;\n    } @else if $width == 'md' {\n        $index: 3;\n    } @else if $width == 'lg' {\n        $index: 4;\n    }  @else if $width == 'xl' {\n        $index: 5\n    }\n    @if $index != 0 {\n        @if $type == 'only' {\n            @if $width == 'xs' {\n                @media only screen and (max-width: nth($breakpoints, $index)) {\n                    @content;\n                }\n            } @else if $width == 'xl' {\n                @media only screen and (min-width: nth($breakpoints, $index - 1)) {\n                    @content;\n                }\n            } @else {\n                @media only screen and (min-width: nth($breakpoints, $index - 1) + 0.001)\n                and (max-width: nth($breakpoints, $index)) {\n                    @content;\n                }\n            }\n        } @else if $type == 'and-below' {\n            @if $width != 'xl' {\n                @media only screen and (max-width: nth($breakpoints, $index)) {\n                    @content;\n                }\n            } @else {\n                @content;\n            }\n        } @else if $type == 'and-above' {\n            @if $width != 'xs' {\n                @media only screen and (min-width: nth($breakpoints, $index - 1) + 0.001) {\n                    @content;\n                }\n            } @else {\n                @content;\n            }\n        } @else {\n            @content;\n        }\n    } @else {\n        @content;\n    }\n  }\n  @mixin pseudo($display: block, $pos: absolute, $content: ''){\n    content: $content;\n    display: $display;\n    position: $pos;\n  }\n  @mixin responsive-ratio($x,$y, $pseudo: false) {\n    $padding: unquote( ( $y / $x ) * 100 + '%' );\n    @if $pseudo {\n        &:before {\n            @include pseudo($pos: relative);\n            width: 100%;\n            padding-top: $padding;\n        }\n    } @else {\n        padding-top: $padding;\n    }\n  }\n  @mixin input-placeholder {\n    &.placeholder { @content; }\n    &:-moz-placeholder { @content; }\n    &::-moz-placeholder { @content; }\n    &:-ms-input-placeholder { @content; }\n    &::-webkit-input-placeholder { @content; }\n  }\n  // Enable hardware acceleration\n  // Ref: https://engageinteractive.co.uk/blog/top-10-scss-mixins\n  @mixin hardware($backface: true, $perspective: 1000) {\n    @if $backface {\n        backface-visibility: hidden;\n    }\n    perspective: $perspective;\n  }\n  @mixin absolute-position ($top: 0, $right: 0, $bottom: 0, $left: 0) {\n    position: absolute;\n    top: $top;\n    right: $right;\n    bottom: $bottom;\n    left: $left;\n  }\n  \n  /* mobile first framework*/\n  .flexbox {\n    display: flex;\n  }\n  .flexbox-inline {\n    display: inline-flex;\n  }\n  .flexbox::before,\n  .flexbox::after,\n  .flexbox-inline::before,\n  .flexbox-inline::after {\n    display: none;\n  }\n  \n  .container {\n    max-width: 100%;\n    margin: 0 auto;\n    padding: 0 10px;\n  }\n  @include screen('sm') {\n    .container {\n        padding: 0 15px;\n    }\n  }\n  @include screen('md') {\n    .container {\n        max-width: 750px;\n        padding: 0 8px;\n    }\n  }\n  @include screen('lg') {\n    .container {\n        max-width: 960px;\n        padding: 0 12px;\n    }\n  }\n  @include screen('xl') {\n    .container {\n        max-width: 1200px;\n        padding: 0 16px;\n    }\n  }\n  \n  a {\n    text-decoration: none !important;\n    color: $theme-black;\n    &:hover{\n        color: #174e96 !important;\n    }\n  }\n  img {\n    max-width: 100%;\n    height: auto;\n  }\n\n  /* header logo */\n.header {\n  max-width: 1200px;\n  margin: 0 auto;\n  .header-logo {\n    height: 100px;\n    border-bottom: 3px solid #111111;\n    justify-content: space-between;\n    align-items: center;\n    display: flex;\n    .fa-envelope {\n      background: black;\n      padding: 0 20px;\n      line-height: 40px;\n      font-weight: 700;\n      letter-spacing: 1px;\n      img {\n        height: 60px;\n        margin: 0 auto;\n      }\n      ::before {\n        content: \"功";\n        color: white;\n      }\n      a {\n        color: white;\n      }\n    }\n  }\n}\n/* menu */\nul {\n  margin: 0;\n  padding: 0;\n  li {\n    flex: 1 1 auto;\n    list-style: none;\n    font-size: 15px;\n    font-weight: 600;\n    a {\n      line-height: 54px;\n      display: block;\n      text-align: center;\n    }\n  }\n}  \n/* header post */\n.header-post{\n  border-top: 3px solid #e8e8e8;\n  border-bottom: 3px solid #e8e8e8;\n  padding: 20px 0;\n  .wrap {\n    margin-top: 0;\n    margin-left: 10px;\n    .title {\n      margin-top: 0;\n      margin-bottom: 10px;\n    }\n    .date {\n      font-size: 11px;\n      ::before {\n        content: \"功";\n      }\n    }\n  }\n}\n /* container flexbox section-top */\n.thumb {\n  position: relative;\n  display: inline-block;\n  .link {\n    position: absolute;\n  }\n  span {\n    position: absolute;\n    left: 10px;\n    bottom: 10px;\n    background-color: #111111;\n    padding: 4px;\n    a {\n      color: white;\n      font-size: 11px;\n      letter-spacing: 1.5px;\n    }\n  }\n}\n.left {\n  width: calc(100%/1.5);\n  flex-shrink: 0;\n  .post-top {\n    .thumb {\n      position: relative;\n      display: inline-block;\n      .link {\n        position: absolute;\n      }\n      span {\n        position: absolute;\n        left: 10px;\n        bottom: 10px;\n        background-color: #111111;\n        padding: 4px;\n        a {\n          color: white;\n          font-size: 11px;\n          letter-spacing: 1.5px;\n        }\n      }\n    }\n    h2 {\n      font-size: 34px;\n      margin-top: 34px;\n      margin-bottom: 8px;\n    }\n    .info {\n      color: #808080;\n      font-size: 12px;\n      font-family: \"Work Sans\";\n      text-transform: uppercase;\n    }\n    .abstract {\n      font-family: Droid Serif;\n      margin-top: 22px;\n      margin-bottom: 26px;\n      color: #333333;\n      text-rendering: optimizeLegibility;\n      line-height: 24px;\n      font-size: 15px;\n      a {\n        background: #111;\n        color: #fff !important;\n        padding: 4px 16px;\n        display: inline-block;\n        text-transform: uppercase;\n        letter-spacing: .05em;\n        font-size: 11px;\n        margin-top: 15px;\n      }\n    }\n  }\n  .post-list {\n    border-top: 1px solid #eee;\n    flex-wrap: wrap;\n    padding-top: 22px;\n    padding-bottom: 12px;\n    a {\n      padding-left: 18px;\n      font-family: Libre Baskerville;\n      font-size: 14px;\n      font-weight: 700;\n      margin-bottom: 5px;\n      line-height: 22px;\n    }\n  }\n  .post-list-slide {\n    border-top: 1px solid #eee;\n    padding-top: 32px;\n    padding-bottom: 16px;\n    .post {\n      position: relative;\n      padding-right: 30px;\n      flex: 1 1 auto;\n      h4 {\n        font-size: 16px;\n        font-weight: 700;\n        margin: 12px 0 8px;\n        font-family: Libre Baskerville;\n      }\n      .info {\n        color: #808080;\n        font-size: 12px;\n        font-family: \"Work Sans\";\n        text-transform: uppercase;\n      }\n      .link-cate {\n        background: #111;\n        color: #fff !important;\n        padding: 4px 16px;\n        display: inline-block;\n        text-transform: uppercase;\n        letter-spacing: .05em;\n        font-size: 11px;\n        position: absolute;\n        top: 10px;\n        left: 10px;\n      }\n    }\n  } \n  .post-list-card {\n    border-top: 1px solid #eee;\n    margin-top: 24px;\n    .post {\n      padding-bottom: 30px;\n      margin-top: 20px;\n    }\n    .thumb {\n      flex-shrink: 0;\n      max-width: 40%;\n      padding-right: 30px;\n      position: relative;\n      img {\n        width: 100%;\n        height: auto;\n      }\n    }\n    .other {\n      .title {\n        margin-top: 0;\n        font-family: \"Libre Baskerville\";\n      }\n      .info {\n        color: #808080;\n        font-size: 12px;\n        font-family: \"Work Sans\";\n        text-transform: uppercase;\n      }\n      .abstract {\n        font-family: \"Droid Serif\";\n        color: #333333;\n        line-height: 24px;\n        font-size: 15px;\n      }\n    }\n  }\n}\n\n"},"lineCount":null}},"error":null,"hash":"ae52fa7afbffef83f5725c7a8d3d61df","cacheData":{"env":{}}}